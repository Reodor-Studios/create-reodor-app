{
  "private": true,
  "scripts": {
    "scaffold": "tsx scripts/scaffold.ts",
    "scaffold:status": "tsx scripts/scaffold.ts status",
    "scaffold:reset": "tsx scripts/scaffold.ts reset",
    "type:check": "tsc --noEmit",
    "with-env": "dotenv -e .env --",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ensure:docker": "sh scripts/ensure-docker.sh",
    "seed:sync": "bunx @snaplet/seed sync",
    "seed": "bunx tsx seed/seed.ts > supabase/seed.sql",
    "db:start": "bun ensure:docker && supabase start",
    "db:diff": "bun ensure:docker && supabase db diff --local --debug -f",
    "migrate:up": "bun ensure:docker && supabase migration up",
    "db:push": "supabase db push",
    "db:reset": "bun ensure:docker && supabase db reset",
    "db:reset:no-seed": "bun ensure:docker && supabase db reset --no-seed",
    "db:types": "bun ensure:docker && supabase gen types typescript --local --schema public > types/database.types.ts",
    "db:types:zod": "supazod -i types/database.types.ts -o schemas/database.schema.ts -s public",
    "remove:public:prefix": "tsx scripts/remove-public-prefix.ts",
    "db:emails:export": "sh scripts/export-email-templates.sh",
    "db:functions:deploy": "supabase functions deploy",
    "db:load:secrets": "sh supabase secrets set --env-file ./supabase/.env",
    "gen:types": "bun run db:types && bun run db:types:zod && bun run remove:public:prefix",
    "email:dev": "sh scripts/ensure-transactional-deps-installed.sh && cd transactional && bun run dev",
    "email:build": "sh scripts/ensure-transactional-deps-installed.sh && cd transactional && bun run build",
    "email:export": "sh scripts/ensure-transactional-deps-installed.sh && cd transactional && bun run export",
    "email:sync": "bun run with-env bun scripts/sync-email-template.sh",
    "railway:setup": "sh scripts/railway-setup.sh",
    "railway:push-env": "sh scripts/railway-push-env.sh",
    "auth:google": "sh scripts/configure-google-auth.sh"
  },
